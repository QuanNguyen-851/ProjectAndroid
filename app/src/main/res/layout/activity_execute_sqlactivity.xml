<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".ExecuteSQLActivity">
<EditText
    android:id="@+id/querySql"
    android:text="SELECT name, type FROM PRAGMA_TABLE_INFO('profile');"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    />
    <TextView
        android:id="@+id/queryResult"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="listTableName"
        />
    <Button
        android:id="@+id/querySqlBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Execute"
        android:layout_marginBottom="20dp"
        />
    <Button
        android:id="@+id/tb_profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="tb_profile"
        />
    <Button
        android:id="@+id/tb_category"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="tb_category"
        />
    <Button
        android:id="@+id/tb_history"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="tb_history"
        />
    <ImageView
        android:id="@+id/testImage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
</LinearLayout>